<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #0C0C0D; -fx-pref-height: 500; -fx-pref-width: 700;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GameOverController">
    <children>
        <Text fill="#f25bb0" layoutX="243.0" layoutY="130.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Game Over" textAlignment="CENTER">
            <font>
                <Font size="54.0" />
            </font>
            <effect>
                <DropShadow>
                    <color>
                        <Color red="0.75" green="0.2708333432674408" blue="0.2708333432674408" />
                    </color>
                </DropShadow>
            </effect>
        </Text>
        <Button fx:id="gameButton" layoutX="277.0" layoutY="189.0" mnemonicParsing="false" onAction="#navigateGame" style="-fx-background-color: #292A2D; -fx-graphic-text-gap: 44;" text="Play Again" textFill="#f25bb0">
            <font>
                <Font size="28.0" />
            </font></Button>
        <Button fx:id="mainMenuButton" layoutX="277.0" layoutY="272.0" mnemonicParsing="false" onAction="#navigateMainMenu" style="-fx-background-color: #292A2D; -fx-graphic-text-gap: 44;" text="Main Menu" textFill="#f25bb0">
            <font>
                <Font size="28.0" />
            </font></Button>
        <Button fx:id="highScoresButton" layoutX="267.0" layoutY="355.0" mnemonicParsing="false" onAction="#navigateHighScores" style="-fx-background-color: #292A2D;" text="High Scores" textAlignment="CENTER" textFill="#f25bb0">
            <font>
                <Font size="28.0" />
            </font></Button>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</AnchorPane>
